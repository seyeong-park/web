<script type="text/javascript">

//@함수의 인자는 갯수를 체크하지 않는다.
function mul(first, second){
	return first*second;
}
document.write('곱은'+mul(3,4)+"<br/>");
document.write('곱은'+mul(3,4,5)+"<br/>");

</script>
