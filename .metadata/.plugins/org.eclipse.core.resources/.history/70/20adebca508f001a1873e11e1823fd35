	var h = $('#here');
	h.click(function(){
		h.text('클릭하면 내용이 변경가능');
	});
 	h.mouseenter(function(){
 		h.text('마우스가 들어왔습니다.');
 	});
	 h.hover(function(){
		 'border','3px solid red'
	 },function(){
		 'border','3px dashed blue'
	 });
});
	
	//마우스 들어왔을때 마우스가 들어왔습니다.
	
	
	
