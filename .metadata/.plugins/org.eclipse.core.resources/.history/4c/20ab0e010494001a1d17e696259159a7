package temp;

import java.beans.Statement;
import java.sql.Connection;
import java.sql.DriverManager;
import java.sql.PreparedStatement;

public class TempDAO {
	String url = "jdbc:oracle:thin:@192.168.0.16:1521:orcl";
	String user = "seyeong";
	String pass = "1234";

	public TempDAO() throws ClassNotFoundException {
		// 1. 드라이버 로딩
		Class.forName("oracle.jdbc.OracleDriver");
		user = "seyeong";
		pass = "1234";

	}
	
	public void insert(TempVO vo) {
		Connection con=null;
		try {
			// 2. 연결객체 얻어오기
			con = DriverManager.getConnection(url, user, pass);
			// 3. sql문장 (insert문)
			String sql = "";
			// 4. 전송객체 얻어오기 (PreparedStatement) + ? 지정
			PreparedStatement st = con.prepareStatement(sql); // 선언된 값 가져오기
			st.setString(1, vo.getId());
			st.setString(2, vo.getPass());
			st.setString(3, vo.getName());
			st.setString(4, vo.getTel());
			st.setString(5, vo.getAddr());
			// 5. 전송(excuteUpdate() 이용)
			int result = st.executeUpdate();

			// 6. 닫기
			st.close();
			
		} catch (Exception e) {
			e.printStackTrace();
		} finally {
			try {
				con.close();
			} catch (Exception e) {
				e.printStackTrace();
			}
		}
	}

}
