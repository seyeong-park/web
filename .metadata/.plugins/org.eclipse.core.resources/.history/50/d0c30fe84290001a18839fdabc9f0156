

$(function(){
	
	
	
	//var count= $('.menuCount').val();
	
	

	//주문목록(이름, 수량, 가격)
	$('.menuCount').change(function(){
		var price= $('.manu:nth-child(3)').val();
//		$('#listrr').text($('.menu:nth-child(2)').val()+$('.menu:nth-child(3)').val())
		$('#listrr:nth-child(1)').append(price);
		
	});
	
	
	
	
	
	
	
	
	
	
	
	
	
});